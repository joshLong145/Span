{
  "lock": "deno-lock.json",
  "tasks": {
    "fmt": "deno fmt",
    "bench": "deno bench benchmark/ --unstable --no-check --allow-all && cd benchmark/hyperfine && ./run.sh",
    "test": "deno test -A --parallel --trace-ops --no-check",
    "ok": "deno fmt && deno task test --coverage=.coverage && deno coverage --exclude=test --exclude=benchmark --exclude=public .coverage",
    "gen-npm": "deno run -A --no-check scripts/gen-npm.ts"
  },
  "exclude": [
    "./examples/*.ts",
    "./lib/*.js",
    "./public/*.js",
    "./scripts/*.ts"
  ],
  "publish": {
    "include": [
      "src",
      "docs/concepts.png",
      "deno.json",
      "README*.md",
      "LICENSE"
    ]
  },
  "lint": {
    "exclude": [
      "npm",
      "public",
      "lib",
      "scripts"
    ]
  },
  "fmt": {
    "exclude": [
      "npm",
      "public",
      "lib",
      "scripts"
    ]
  },
  "name": "@joshlong145/span",
  "version": "0.7.0",
  "exports": "./src/mod.ts"
}
